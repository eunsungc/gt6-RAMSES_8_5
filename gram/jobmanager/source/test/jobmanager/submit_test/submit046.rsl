&(executable = /bin/sh)
 (arguments = -c 'echo stderr 1>&2; echo stdout;')
 (scratch_dir=$(HOME))
 (directory=$(SCRATCH_DIRECTORY))
 (stdout = $(SCRATCH_DIRECTORY) # /stdout)
 (stderr = $(SCRATCH_DIRECTORY) # /stderr)
